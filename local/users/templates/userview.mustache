<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">First Name</th>
                <th scope="col">Last Name</th>
                <th scope="col">Email</th>
                <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody>
        {{#data}}
            <tr>
                <td>{{ firstname }}</td>
                <td>{{ lastname }}</td>
                <td>{{ email }}</td>
                <td>
                    <span class="text-light bg-danger pr-1 pl-1 rounded-circle" data-action="deleteuser" data-id={{ id }}><i class="fa fa-trash-o" aria-hidden="true"></i></span>
                </td>
            </tr>
        {{/ data}}
        {{^data}}
            <tr>
                {{#str}}nodataavailable, local_users{{/str}}
            </tr>
        {{/data}}
        </tbody>
    </table>
</div>